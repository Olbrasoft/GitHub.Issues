{
  "SecureStore": {
    "SecretsPath": "~/.config/github-issues/secrets/secrets.json",
    "KeyPath": "~/.config/github-issues/keys/secrets.key"
  },
  "ConnectionStrings": {
    "DefaultConnection": "<from-user-secrets>"
  },
  "TextTransformation": {
    "Embeddings": {
      "Provider": "Cohere",
      "Model": "embed-multilingual-v3.0",
      "Dimensions": 1024,
      "Cohere": {
        "Model": "embed-multilingual-v3.0",
        "Dimensions": 1024
      },
      "Ollama": {
        "BaseUrl": "http://localhost:11434",
        "MaxStartupRetries": 30,
        "StartupRetryDelayMs": 1000
      }
    }
  },
  "GitHub": {
    "Token": "<from-user-secrets>",
    "Owner": "Olbrasoft",
    "OwnerType": "user",
    "IncludeArchived": false,
    "IncludeForks": false,
    "Repositories": []
  },
  "Sync": {
    "GitHubApiPageSize": 100,
    "BatchSaveSize": 100,
    "MaxEmbeddingTextLength": 8000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  }
}
